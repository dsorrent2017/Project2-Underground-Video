<div class="container mt-4">
    <div class="row">
        <div class="col-md-4 mx-auto" id="registerBox">
            <h1 class="text-center">Register</h1>
            <form class="alertAppendReg">
                <div class="form-group">
                    <label for="emailInputReg">Email address</label>
                    <input type="email" class="form-control" id="emailInputReg" aria-describedby="emailHelp">
                </div>
                <div class="form-group">
                    <label for="passwordInputReg">Password</label>
                    <input type="password" class="form-control" id="passwordInputReg">
                </div>
                <div class="form-group">
                    <label for="passwordTwoInputReg">Confirm Password</label>
                    <input type="password" class="form-control" id="passwordTwoInputReg">
                </div>
                <button id="submitBtnReg" type="submit" class="btn btn-secondary">Submit</button>
                <small class="form-text text-muted">Already Registered..? <a href="/user/login"
                        style="color: whitesmoke;">Log In</a></small>
            </form>
        </div>
    </div>
